<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Task7Main</display-name>
	<servlet>
		<servlet-name>appl-controller</servlet-name>
		<servlet-class>controller.Controller</servlet-class>
		<init-param>
			<param-name>jdbcDriverName</param-name>
			<param-value>   com.mysql.jdbc.Driver   </param-value>
		</init-param>
		<init-param>
			<param-name>jdbcURL</param-name>
			<param-value>   jdbc:mysql:///test    </param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /transitionDay                     </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /createCustomerAccount                     </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /createFund                    </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /depositCheck                     </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /requestCheck                     </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /login                     </url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>appl-controller</servlet-name>
		<url-pattern>    /logout                     </url-pattern>
	</servlet-mapping>
</web-app>